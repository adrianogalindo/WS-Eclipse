public class Entradadedados {

	public static void main(String[] args) {

		Scanner sc = new Scanner(System.in);
		
		string x;
		// next serve para ler texto
		int y;
		double z;
		
		y = sc.nextInt();
		z = sc.nextDouble();
		sc.useLocale(Locale.ENGLISH);
		
		char u;
		u = sc.next().charAt(0);
		// chartAt pega o caractere, nesse caso, pegará o primeiro caractere.
	
		sc.close();
	}

}
