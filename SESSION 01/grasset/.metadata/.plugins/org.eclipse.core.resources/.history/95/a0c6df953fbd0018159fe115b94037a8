package ca.qc.grasset._420_pa4_ag.tp01;

import static org.junit.Assert.*;

import org.junit.Assert;
import org.junit.Test;

public class UtilitaireTableauTest_Test1 {

	@Test
	public void test() {
	
        final int[] x = { 2, 3, 5 };

        final int y = 2;
        
        int indicateur = new UtilitaireTableau().chercherDernierePositionValeur(x, y);

        if (indicateur >= 0) {
            System.out.println("La position de " + y + " est " + indicateur);
        } else {
        	System.out.println("La position de " + y + " n'est pas trouv√©e. Retourne " + indicateur);
        }

        Assert.assertTrue(indicateur == 0);
    }

}
	
	